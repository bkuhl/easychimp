{
    "name": "bkuhl/easychimp",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Kuhl",
            "email": "benkuhl@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Easychimp\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "pacely/mailchimp-apiv3": "~1.0",
        "illuminate/support": ">=5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~3.1",
        "symfony/var-dumper": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Easychimp\\ServiceProvider"
            ],
            "aliases": {
                "Easychimp": "Easychimp\\MailchimpFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
